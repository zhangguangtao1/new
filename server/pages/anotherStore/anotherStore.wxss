/* pages/store/store.wxss */
.store{padding:0 30rpx;}
#map{width: 100%; height: 480rpx}
.storeItem{margin:50rpx 0;overflow:hidden;}
.storeItem text{float:left;font-size:40rpx;margin-right:42rpx}
.storeText{overflow:hidden;white-space:nowrap;text-overflow:ellipsis;font-size:30rpx;}
.telpnone text{margin-right:0rpx;}
.yuyue{margin-top:70rpx;line-height:100rpx;margin:0 -30rpx;background-color:#f2f8fe;padding:0 30rpx;font-size:30rpx;}
.yuyue text{display:inline-block;vertical-align:top;font-size:40rpx;margin-right:40rpx;}

.innerLogo{text-align:center;margin-bottom: 120rpx;}
.innerLogo image:first-child{display:block;width:100%;height:230rpx;}
.myLogo{position:absolute;margin-left:-70rpx;width:140rpx;height:140rpx;margin-top:-70rpx;}


