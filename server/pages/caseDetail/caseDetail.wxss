/* pages/caseDetail/caseDetail.wxss */
page{background-color:#f5f5f5;}
.detailImg{display:block;width:100%;height:420rpx;}
.detailName{line-height:110rpx;background-color:#ffffff;border-bottom:1rpx solid #dedede;padding:0 30rpx;}
.detailName text{float:right;font-size:30rpx;color:#2280f0;margin-left:20rpx;}
.detailName text::first-letter{font-size:26rpx;}
.detailLeft{font-size:34rpx;overflow:hidden;text-overflow:ellipsis;white-space:nowrap;}
/* 服务介绍 */
.serverExplain{margin-top:20rpx;padding:30rpx 25rpx 216rpx;background-color:#ffffff;outline:#ffffff solid 99999rpx;position:absolute;clip:rect(0 99999rpx 99999rpx 0);}
.serverTitle{font-size:36rpx;color:#333333;}
.serverTitle::before{content:'';height:40rpx;border:6rpx solid #227ff0f5;border-radius:6rpx;margin-right:15rpx}
.editorCon{font-size:24rpx;color:#333333;line-height:50rpx;margin-top:35rpx;}
.serverFooter{line-height:106rpx;background-color:#f0f0f0;overflow:hidden;position:fixed;bottom:0;left:0;right:0;}
.serverFooter .left{float:left;width:50%;text-align:center;font-size:30rpx;color:#999999;}
.serverFooter .left text{font-size:40rpx;vertical-align:top;display: inline-block;}
.serverFooter .right{float:left;width:50%;text-align:center;font-size:30rpx;background-color:#2280f0;color:#ffffff;}








